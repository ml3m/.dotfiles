#!/bin/bash

logo=(
'       _'
'       \`*-.'
'        )  _`-.'
'       .  : `. .'
'       : _   '\''  \\'
'       ; *` _.   `*-._'
'       `-.-'\''          `-.'
'         ;       `       `.'
'         :.       .        \'
'         . \\  .   :   .-'\''   .'
'         '\''  `+.;  ;  '\''      :'
'         :  '\''  |    ;       ;-.'
'         ; '\''   : :`-:     _.`* ;'
'[bug] .*'\'' /  .*'\'' ; .*`- +'\''  `*'\'''
'      `*-*   `*-*  `*-*'\'''
)

for line in "${logo[@]}"; do
  echo -e "$line"
done
